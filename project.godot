; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NewtonTemplate"
run/main_scene="res://Scenes/empty_newton_level.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

VRUserMeasurements="*res://addons/Godot-VR-Measurements/scenes/VRUserMeasurements.tscn"

[dotnet]

project/assembly_name="NewtonTemplate"

[editor_plugins]

enabled=PackedStringArray("res://addons/Godot-OpenXR-Startup/plugin.cfg", "res://addons/Godot-VR-Measurements/plugin.cfg", "res://addons/Godot-VR-UI/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[physics]

3d/physics_engine="JoltPhysics3D"
3d/solver/solver_iterations=24
jolt_3d/collisions/areas_detect_static_bodies=true
jolt_3d/solver/velocity_iterations=20
jolt_3d/solver/position_iterations=10
jolt_3d/solver/position_correction=50.0

[rendering]

renderer/rendering_method="mobile"

[xr]

openxr/enabled=true
shaders/enabled=true
